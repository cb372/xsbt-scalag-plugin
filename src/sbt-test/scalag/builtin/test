> g class A
$ exists src/main/scala/A.scala
> generate class B
$ exists src/main/scala/B.scala

> g object C
$ exists src/main/scala/C.scala
> g object A
$ newer src/main/scala/C.scala src/main/scala/A.scala

> g specs2 E
$ exists src/test/scala/ESpec.scala
> g specs2 F unit
$ exists src/test/scala/FSpec.scala
> g specs2 G acceptance
$ exists src/test/scala/GSpec.scala
> g specs2 H hoge
-$ exists src/test/scala/HSpec.scala

